{
  "name": "ludo2go-admin",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development node app.js",
    "start": "NODE_ENV=production node app.js",
    "build": "mkdir -p .adminjs && prisma generate",
    "postinstall": "mkdir -p .adminjs && prisma generate",
    "clean": "rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean && npm install",
    "vercel-build": "mkdir -p .adminjs && prisma generate"
  },
  "prisma": {
    "schema": "node_modules/@jssprz/ludo2go-database/prisma/schema.prisma"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@adminjs/prisma": "^5.0.4",
    "@jssprz/ludo2go-database": "^0.1.1",
    "@prisma/client": "^6.19.0",
    "adminjs": "^7.8.17",
    "express": "^4.21.0",
    "express-session": "^1.18.2",
    "prisma": "^6.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-syntax-import-assertions": "^7.27.1"
  },
  "overrides": {
    "rollup": "npm:@rollup/wasm-node"
  }
}
