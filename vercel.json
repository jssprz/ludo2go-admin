{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "env": {
    "NODE_VERSION": "18.x"
  },
  "buildCommand": "npm ci --omit=optional",
  "installCommand": "npm ci --omit=optional"
}
